/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "ar9331.dtsi"

/ {
	model = "TP-Link TL-WR741ND v4";
	compatible = "tplink,tl-wr740n";

	aliases {
		serial0 = &uart;
	};

	chosen {
		bootargs = "machtype=DTB console=ttyATH0,115200";
	};

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x2000000>;
	};

	leds {
		compatible = "gpio-leds";

		lan1 {
			label = "tp-link:green:lan1";
			gpios = <&gpio 14 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		lan2 {
			label = "tp-link:green:lan2";
			gpios = <&gpio 15 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		lan3 {
			label = "tp-link:green:lan3";
			gpios = <&gpio 16 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		lan4 {
			label = "tp-link:green:lan4";
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		qss {
			label = "tp-link:green:qss";
			gpios = <&gpio 1 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		system {
			label = "tp-link:green:system";
			gpios = <&gpio 27 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		wan {
			label = "tp-link:green:wan";
			gpios = <&gpio 13 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};

		wlan {
			label = "tp-link:green:wlan";
			gpios = <&gpio 0 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
	};

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		button@0 {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 11 GPIO_ACTIVE_HIGH>;
		};

		button@1 {
			label = "wps";
			linux,code = <KEY_WPS_BUTTON>;
			gpios = <&gpio 26 GPIO_ACTIVE_HIGH>;
		};
	};
};

&ref {
	clock-frequency = <25000000>;
};

&uart {
	status = "okay";
};

&gpio {
	status = "okay";
};

&spi {
	num-chipselects = <1>;
	status = "okay";

	/* EON EN25Q32B SPI flash */
	spiflash: en25q32b@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "eon,en25q32b", "jedec,spi-nor";
		spi-max-frequency = <104000000>;
		reg = <0>;

		linux,part-probe = "tp-link";

		part_uboot: partition@0 {
			label = "u-boot";
		};

		part_art: partition@3f0000 {
			label = "art";
		};
	};
};

&wifi0 {
	status = "okay";
	mtd-eeprom = <&part_art 0x1000>;
	mtd-mac-address = <&part_uboot 0x1fc00>;
};

&mdio {
	status = "okay";
	phy-mask = <0x00>;
};

&eth0 {
	status = "okay";
	mtd-mac-address = <&part_uboot 0x1fc00>;
	mtd-mac-address-increment = <(-1)>;
};

&eth1 {
	status = "okay";
	mtd-mac-address = <&part_uboot 0x1fc00>;
	mtd-mac-address-increment = <(1)>;
};

